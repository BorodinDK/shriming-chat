match(this.ctx.data)
    .def()(function() {
    var data = this.ctx.data;

    return applyCtx({
        block: 'page',
        head: [{elem: 'css', url: '/views/desktop.bundles/merged/_merged.css'}],
        title: data.title,
        content: [
            {
                block: 'controller',
                elem: data.options.controller,
                mods: {action: data.options.action},
                data: data
            },
            {elem: 'js', url: '/views/desktop.bundles/merged/_merged.js'}
        ]
    });
});
